<script>
$( document ).ready(function() {
  $("#submit").click( function(){
    applyCollab(<%-req.session.userId%>);
  });
});

  function applyCollab(userId){
    var request = $.ajax({
      method: "PATCH",
      url: "/collaboration/<%- collab.id %>",
      data: {
          <% // for (var i in owner_ids){ %>
          owners: [userId,<%- owner_ids %>]
          <% //} %>
          //owners: userId
      },
      dataType: "json"
    });

    request.done(function( collab ) {
      alert("added you to the collab!");
    });

    request.fail(function (jqXHR, textStatus) {
          alert("collab update failed! " + textStatus);
    });
  }

</script>
